{#
  @var FormViewInterface form
#}
{% extends 'layout.html.twig' %}

{% block title %}{{'settings'|trans}}{% endblock %}

{% block content %}
  {% import '_bootstrap/button.html.twig' as btn %}
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-12">
        {{form_start(form)}}
          {{form_rest(form)}}
          {{btn.submit(null, 'save')}}
        {{form_end(form)}}
      </div>
    </div>
  </div>
{% endblock %}
